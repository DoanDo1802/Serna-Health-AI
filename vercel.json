{
  "version": 2,
  "builds": [
    {
      "src": "frontend_v2/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend_v2/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@next_public_supabase_anon_key"
  },
  "buildCommand": "cd frontend_v2 && npm run build",
  "outputDirectory": "frontend_v2/.next",
  "installCommand": "cd frontend_v2 && npm install"
}

